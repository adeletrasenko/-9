my_dict={"Denis":18,"Adele":20,"Carina":19}
print("Dict:",my_dict)
print("Existing value:", my_dict["Denis"])
print("Not existing value:",my_dict.get("Nonna"))
del my_dict["Adele"]
print("Deleted value: Adele")
(my_dict.update({"Valera":30,"Maksim":45}))
print('Modified dictionary: ',my_dict)

my_set={"Peach",1,2.44}
print("Set:",my_set)
my_set.add(13.3)
my_set.discard(1)
print("Modified set: ",my_set)